{
  "type": "container",
  "resources": [
    {
      "stateMachine": "package",
      "sm_args": "rabbitmq-server",
      "sm_state": "installed"
    },
    {
      "stateMachine": "cmd",
      "sm_args": "rabbitmqctl add_user guest password"
    },
    {
      "stateMachine": "cmd",
      "sm_args": "rabbitmqctl set_permissions openstack \".*\" \".*\" \".*\""
    }
  ]
}